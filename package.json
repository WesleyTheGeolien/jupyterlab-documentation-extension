{
  "name": "jupyterlab-documentation-extension",
  "version": "0.1.0",
  "description": "generate python documentation and add to help jupyterlab help menu",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/WesleyTheGeolien/jupyterlab-documentation-extension",
  "bugs": {
    "url": "https://github.com/WesleyTheGeolien/jupyterlab-documentation-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Wesley Banfield",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/WesleyTheGeolien/jupyterlab-documentation-extension.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.0",
    "@jupyterlab/apputils": "^0.18.4",
    "@jupyterlab/mainmenu": "^0.7.4",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/widgets": "^1.6.0",
    "clean": "^4.0.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
